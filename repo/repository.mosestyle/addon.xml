<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.mosestyle"
       name="Mosestyle Repository"
       version="1.0.0"
       provider-name="Mosestyle">
  <requires>
    <import addon="xbmc.addon" version="17.0.0"/>
  </requires>
  <extension point="xbmc.addon.repository" name="Mosestyle Repo">
    <dir>
      <info compressed="false">https://raw.githubusercontent.com/mosestyle/kodis/gh-pages/repo/zips/addons.xml</info>
      <checksum>https://raw.githubusercontent.com/mosestyle/kodis/gh-pages/repo/zips/addons.xml.md5</checksum>
      <datadir zip="true">https://raw.githubusercontent.com/mosestyle/kodis/gh-pages/repo/zips/</datadir>
    </dir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Repository for Mosestyle Build Installer</summary>
    <description>Provides the Mosestyle Build Installer add-on and updates.</description>
    <platform>all</platform>
  </extension>
</addon>
